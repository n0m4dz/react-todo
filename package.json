  "description": "React Todo Applciation",
  "main": "server.js",
  "scripts": {
    "start": "npm run env:win && npm run start:win",
    "env:win": "set NODE_ENV=development && set BABEL_ENV=development",
    "start:win": "node ./bin/compile.js",
    "build": "npm run prod:win && npm run build:win",
    "prod:win": "set NODE_ENV=production && set BABEL_ENV=production",
    "build:win": "webpack -p --progress --colors --config ./bin/compile.production.js",
    "start-php": "cd backend && php -S localhost:8000"
  },
